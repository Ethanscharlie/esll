component Color
  Integer r
  Integer g
  Integer b
end

component Box
  Integer x
  Integer y
  Integer w
  Integer h
end

system RenderSystem(Color, Box) 
  fill(Color:r, Color:g, Color:b)
  drawRectangle(Box:x, Box:y, Box:w, Box:h)
end

