String checkNumber(Integer number)
  if number % 3 = 0
    return "fizz"
  elif number % 5 = 0
    return "buzz"
  elif number % 5 = 0 & number % 3 = 0
    return "fizzbuzz"
  end

  return str(number)
end

Void doFizzbuzz(Integer number)
  _ print(number)
  _ print(checkNumber(number))
  _ print("")
end

Void main()
  _ doFizzbuzz(5)
  _ doFizzbuzz(3)
  _ doFizzbuzz(15)
  _ doFizzbuzz(2)
end

