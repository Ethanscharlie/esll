String checkNumber(Integer number)
  if number % 3 = 0
    return "fizz"
  elif number % 5 = 0
    return "buzz"
  elif number % 5 = 0 & number % 3 = 0
    return "fizzbuzz"
  end

  return number
end

Void doFizzbuzz(Integer number)
  print(number)
  print(checkNumber(number))
  print("")
end

Void main()
  Integer makeVariableTest 3
  doFizzbuzz(5)
  doFizzbuzz(3)
  doFizzbuzz(15)
  doFizzbuzz(2)
end

